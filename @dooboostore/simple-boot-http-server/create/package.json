{
  "name": "@dooboostore/create-simple-boot-http-server",
  "description": "Create a simple-boot-http-server or Sapper site in a single command.",
  "version": "1.0.1",
  "author": "dooboostore",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create-simple-boot-http-server": "./bin/run",
    "create-simple-boot-http-server-app": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "degit": "^2.1.3",
    "inquirer": "^6.3.1",
    "p-event": "^4.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/inquirer": "^6.0.1",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "chai": "^4",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "nyc": "^15.1.0",
    "ts-node": "^8",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/dooboostore-develop/packages",
  "keywords": [
    "oclif",
    "simple-boot",
    "sapper",
    "create-simple-boot"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "create-simple-boot"
  },
  "repository": "https://github.com/dooboostore-develop/packages",
  "scripts": {
    "dev": "NODE_ENV=development node ./bin/run",
    "build": "rm -rf lib && tsc --outDir lib --declarationDir lib && tsc",
    "publish:npm": "npm run build && npm publish",
    "posttest": "tslint -p test -t stylish",
    "fix": "tslint -p test -t stylish --fix",
    "prepack": "tsc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
