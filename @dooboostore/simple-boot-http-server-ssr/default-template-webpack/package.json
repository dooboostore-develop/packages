  "bugs": {
    "url": "https://github.com/visualkhh/simple-boot-http-ssr",
    "email": "visualkhh@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visualkhh/simple-boot-http-ssr"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "scripts": {
    "dev:serve": "npm run serve --workspace=dev",
    "tsc:watch": "rm -rf ./dist && mkdir dist && cp package.json dist && tsc --watch --sourceMap true",
    "tsc:watch:dev": "rm -rf ./dist && mkdir dist && cp package-dev.json dist && tsc --watch --sourceMap true",
    "tsc:watch:prod": "rm -rf dist && mkdir dist && cp package-prod.json dist/package.json && cp README.MD dist && tsc --watch  --sourceMap true --outDir dist --declarationDir dist",
    "typecheck": "tsc --noEmit",
    "backend:start": "node dist-back-end/index.js",
    "backend:build": "webpack --config back-end/webpack.config.js",
    "backend:inspect:run": "pnpm run front:build && pnpm run backend:build && node --inspect dist-back-end/index.js",
    "backend:clinic:heap:run": "clinic heap -- node ./dist-back-end/index.js",
    "backend:clinic:bubbleprof:run": "clinic bubbleprof -- node ./dist-back-end/index.js",
    "backend:clinic:doctor:run": "clinic doctor -- node ./dist-back-end/index.js",
    "backend:clinic:flame:run": "clinic flame -- node ./dist-back-end/index.js",
    "backend:clinic:heapprofiler:run": "clinic heapprofiler -- node ./dist-back-end/index.js",
    "backend:build:watch": "webpack --config back-end/webpack.config.js --watch",
    "backend:build:no_sync:watch": "MODE=NO_SYNC webpack --config back-end/webpack.config.js --watch",
    "backend:build:only_sync:watch": "MODE=ONLY_SYNC webpack --config back-end/webpack.config.js --watch",
    "frontend:build": "webpack --config front-end/webpack.config.js",
    "frontend:build:watch": "webpack --config front-end/webpack.config.js --watch",
    "frontend:dev": "webpack serve --config front-end/webpack.config.js",
    "frontend:no_reload:dev": "webpack serve --config front-end/webpack.config.js --no-live-reload",
    "frontend:start": "http-server dist-front",
    "build": "pnpm run frontend:build && pnpm run backend:build",
    "start": "pnpm run build && pnpm run backend:start",
    "test:build": "webpack --config test/webpack.config.js",
    "test:build:watch": "webpack --config test/webpack.config.js --watch",
    "test:start": "node dist-test/index.js",
    "test:front:dev": "webpack serve --config test/front.webpack.config.js",
    "test:front:build": "webpack --config test/front.webpack.config.js"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/css": "^0.0.33",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^21.1.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mime-types": "^2.1.1",
    "@types/mysql": "^2.15.27",
    "@types/node": "^22.13.10",
    "@types/node-schedule": "^1.3.2",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "bufferutil": "^4.0.9",
    "canvas": "^3.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "http-server": "^14.1.1",
    "jest": "^26.6.3",
    "nodemon": "^3.1.10",
    "nodemon-webpack-plugin": "^4.8.2",
    "raw-loader": "^4.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "utf-8-validate": "^6.0.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@dooboostore/core": "latest",
    "@dooboostore/core-node": "latest",
    "@dooboostore/core-web": "latest",
    "@dooboostore/dom-render": "latest",
    "@dooboostore/lib-node": "latest",
    "@dooboostore/lib-web": "latest",
    "@dooboostore/simple-boot": "latest",
    "@dooboostore/simple-boot-front": "latest",
    "@dooboostore/simple-boot-http-server": "latest",
    "@dooboostore/simple-boot-http-server-ssr": "latest",
    "@google/genai": "^1.5.1",
    "canvas": "^3.1.0",
    "jsdom": "^26.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
}
