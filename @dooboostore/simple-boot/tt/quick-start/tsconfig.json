{
  "compilerOptions": {
    "target": "es2019",
    "module": "ESNext",
    "lib": ["es2019", "dom"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "moduleResolution": "bundler"
  }
}
