{
  "name": "@dooboostore/simple-boot/example/router",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild-deps": "pnpm --filter @dooboostore/core build && pnpm --filter @dooboostore/core-node build && pnpm --filter @dooboostore/simple-boot build",
    "dev": "pnpm run prebuild-deps && webpack serve --mode development --config webpack.config.js --watch",
    "build": "pnpm run prebuild-deps && webpack --mode production",
    "ts-node": "pnpm run prebuild-deps && ts-node index.ts",
    "ts-node:watch": "pnpm run prebuild-deps && ts-node-dev --respawn index.ts",
    "start": "pnpm run prebuild-deps && ts-node index.ts",
    "start:watch": "pnpm run prebuild-deps && ts-node-dev --respawn index.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "nodemon-webpack-plugin": "^4.8.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "simple-boot-front-cli": "^1.0.10",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2",
    "@dooboostore/core": "workspace:*",
    "@dooboostore/core-node": "workspace:*",
    "@dooboostore/simple-boot": "workspace:*"
  }
}
