{
  "name": "@dooboostore/app-system/example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "ts-node": "ts-node index.ts",
    "ts-node:watch": "ts-node-dev --respawn index.ts",
    "start": "rollup -c && sbf serve --path ./dist --port 4500",
    "start:watch": "npm run bundle:watch & npm run serve:watch",
    "serve": "sbf serve --path ./dist --port 4500",
    "serve:watch": "sbf serve --path ./dist --port 4500 --watch",
    "serve:proxy": "sbf serve --path ./dist --port 4500 --proxy http://localhost:8080"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "simple-boot-front-cli": "^1.0.10",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "@dooboostore/core": "workspace:*",
    "@dooboostore/core-node": "workspace:*",
    "@dooboostore/core-web": "workspace:*",
    "@dooboostore/dom-render": "workspace:*",
    "@dooboostore/simple-boot": "workspace:*",
    "@dooboostore/simple-boot-front": "workspace:*",
    "@dooboostore/app-system": "workspace:*",
    "reflect-metadata": "^0.2.2"
  }
}
