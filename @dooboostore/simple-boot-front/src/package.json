{
  "name": "@dooboostore/simple-boot-front",
  "version": "1.0.122",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "front end SPA frameworks",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "front",
    "spa",
    "SPA",
    "module",
    "front-end",
    "framwork",
    "node"
  ],
  "author": "dooboostore <dooboostore@gmail.com> (https://github.com/dooboostore)",
  "contributors": [
    "sewoo <sewooimda@nate.com> (https://github.com/sewoo)",
    "SeoDH <adonis_84@naver.com> (https://github.com/SeoDH)",
    "tkyung (https://github.com/tkyungDev)",
    "Hoil-Gang (https://github.com/khl1154)",
    "hwangsero (https://github.com/hwangsero)"
  ],
  "homepage": "https://github.com/dooboostore-develop/packages",
  "bugs": {
    "url": "https://github.com/dooboostore-develop/packages",
    "email": "dooboostore@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dooboostore-develop/packages"
  },
  "scripts": {
    "tsc:build": "rm -rf dist && tsc -p tsconfig.build.json",
    "clean": "rm -rf  dist",
    "npm:build": "pnpm run clean && pnpm run tsc:build && pnpm run bundle && cp README.MD dist/ && pnpm pack && TARBALL_NAME=$(ls dooboostore-simple-boot-front-*.tgz | head -n 1) && tar -xzf \"$TARBALL_NAME\" -C dist --strip-components=1 package/package.json && rm \"$TARBALL_NAME\"",
    "npm:clean": "rm -rf  dist",
    "npm:publish": "pnpm run npm:build && cd dist && pnpm publish --no-git-checks",
    "npm:publish:clean": "pnpm run npm:publish && rm -rf dist",
    "bundle": "webpack --config webpack.config.js",
    "ts:clean": "find . -name '*.js' -o -name '*.map' -o -name '*.d.ts' | xargs rm -f",
    "webpack:dev": "webpack serve --mode development",
    "webpack:build": "webpack --mode production",
    "tsc:watch": "rm -rf ./dist && mkdir dist && cp package.json dist && tsc --watch --outDir dist --declarationDir dist --sourceMap true",
    "build:ts:watch": "rm -rf ./dist && mkdir dist && cp package-prod.json dist/package.json && tsc --watch --outDir dist --declarationDir dist --sourceMap true",
    "test": "jest --detectOpenHandles --forceExit",
    "typecheck": "tsc --noEmit"
  },
  "alias": {},
  "sideEffects": [
    "reflect-metadata"
  ],
  "devDependencies": {
    "@types/node": "^20",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "html-webpack-plugin": "^5.6.3",
    "simple-boot-front-cli": "^1.0.10",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "@dooboostore/core": "workspace:*",
    "@dooboostore/core-node": "workspace:*",
    "@dooboostore/core-web": "workspace:*",
    "@dooboostore/dom-render": "workspace:*",
    "@dooboostore/simple-boot": "workspace:*"
  }
}
