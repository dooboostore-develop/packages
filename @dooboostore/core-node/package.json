{
  "name": "@dooboostore/core-node",
  "version": "1.0.8",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./convert": {
      "types": "./dist/types/convert/index.d.ts",
      "node": "./dist/cjs/convert/index.js",
      "require": "./dist/cjs/convert/index.js",
      "import": "./dist/esm/convert/index.js"
    },
    "./convert/ConvertUtils": {
      "types": "./dist/types/convert/ConvertUtils.d.ts",
      "node": "./dist/cjs/convert/ConvertUtils.js",
      "require": "./dist/cjs/convert/ConvertUtils.js",
      "import": "./dist/esm/convert/ConvertUtils.js"
    },
    "./fetch": {
      "types": "./dist/types/fetch/index.d.ts",
      "node": "./dist/cjs/fetch/index.js",
      "require": "./dist/cjs/fetch/index.js",
      "import": "./dist/esm/fetch/index.js"
    },
    "./fetch/HttpPageDownloader": {
      "types": "./dist/types/fetch/HttpPageDownloader.d.ts",
      "node": "./dist/cjs/fetch/HttpPageDownloader.js",
      "require": "./dist/cjs/fetch/HttpPageDownloader.js",
      "import": "./dist/esm/fetch/HttpPageDownloader.js"
    },
    "./file": {
      "types": "./dist/types/file/index.d.ts",
      "node": "./dist/cjs/file/index.js",
      "require": "./dist/cjs/file/index.js",
      "import": "./dist/esm/file/index.js"
    },
    "./file/FileUtils": {
      "types": "./dist/types/file/FileUtils.d.ts",
      "node": "./dist/cjs/file/FileUtils.js",
      "require": "./dist/cjs/file/FileUtils.js",
      "import": "./dist/esm/file/FileUtils.js"
    },
    "./memory": {
      "types": "./dist/types/memory/index.d.ts",
      "node": "./dist/cjs/memory/index.js",
      "require": "./dist/cjs/memory/index.js",
      "import": "./dist/esm/memory/index.js"
    },
    "./memory/MemoryUtils": {
      "types": "./dist/types/memory/MemoryUtils.d.ts",
      "node": "./dist/cjs/memory/MemoryUtils.js",
      "require": "./dist/cjs/memory/MemoryUtils.js",
      "import": "./dist/esm/memory/MemoryUtils.js"
    },
    "./path": {
      "types": "./dist/types/path/index.d.ts",
      "node": "./dist/cjs/path/index.js",
      "require": "./dist/cjs/path/index.js",
      "import": "./dist/esm/path/index.js"
    },
    "./path/PathUtils": {
      "types": "./dist/types/path/PathUtils.d.ts",
      "node": "./dist/cjs/path/PathUtils.js",
      "require": "./dist/cjs/path/PathUtils.js",
      "import": "./dist/esm/path/PathUtils.js"
    },
    "./process": {
      "types": "./dist/types/process/index.d.ts",
      "node": "./dist/cjs/process/index.js",
      "require": "./dist/cjs/process/index.js",
      "import": "./dist/esm/process/index.js"
    },
    "./process/ProcessUtils": {
      "types": "./dist/types/process/ProcessUtils.d.ts",
      "node": "./dist/cjs/process/ProcessUtils.js",
      "require": "./dist/cjs/process/ProcessUtils.js",
      "import": "./dist/esm/process/ProcessUtils.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "author": "dooboostore <dooboostore@gmail.com> (https://github.com/dooboostore)",
  "contributors": [
    "sewoo <sewooimda@nate.com> (https://github.com/sewoo)",
    "SeoDH <adonis_84@naver.com> (https://github.com/SeoDH)",
    "tkyung (https://github.com/tkyungDev)",
    "Hoil-Gang (https://github.com/khl1154)",
    "hwangsero (https://github.com/hwangsero)"
  ],
  "homepage": "https://github.com/dooboostore-develop/packages",
  "bugs": {
    "url": "https://github.com/dooboostore-develop/packages",
    "email": "dooboostore@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dooboostore-develop/packages"
  },
  "keywords": [
    "util",
    "node"
  ],
  "peerDependencies": {
    "reflect-metadata": "^0.2.2",
    "@dooboostore/core": "workspace:*"
  },
  "devDependencies": {
    "esbuild-plugin-tsc": "^0.5.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "tslib": "^2.8.1",
    "@types/node": "^20",
    "typescript": "^5.9.2",
    "esbuild": "^0.23.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "node build.mjs esm",
    "watch:esm": "pnpm run build:esm && node build.mjs esm --watch",
    "build:cjs": "node build.mjs cjs",
    "watch:cjs": "pnpm run build:cjs && node build.mjs cjs --watch",
    "build": "pnpm run clean && node build.mjs all",
    "build:umd-bundle": "node build.mjs umd-bundle",
    "watch:umd-bundle": "pnpm run build:umd-bundle && node build.mjs umd-bundle --watch",
    "build:esm-bundle": "node build.mjs esm-bundle",
    "watch:esm-bundle": "pnpm run build:esm-bundle && node build.mjs esm-bundle --watch",
    "watch": "node build.mjs all --watch",
    "publish:npm": "pnpm run build && cp package.json package.json.back && pnpm pack && TARBALL_NAME=$(ls dooboostore-core-node-*.tgz | head -n 1) && tar -xzf \"$TARBALL_NAME\" -C . --strip-components=1 package/package.json && rm \"$TARBALL_NAME\" && (pnpm publish --no-git-checks || true) && cp package.json.back package.json && rm -rf package.json.back  ",
    "ts:clean": "find . -name '*.js' -o -name '*.map' -o -name '*.d.ts' | xargs rm -f",
    "typecheck": "pnpm exec tsc --noEmit",
    "test": "echo test"
  }
}
